algoritmo "Formulário"

var
nome: caractere
idade: inteiro
salario: real

inicio
escreva("Informe seu nome: ")
leia(nome)

escreva("Informe sua idade: ")
leia(idade)

escreva("Informe seu salário: ")
leia(salario)

escreval("O nome informado foi:    ", nome)
escreval("A idade informada foi:   ", idade)
escreval("O salário informado foi: ", salario)

fimalgoritmo